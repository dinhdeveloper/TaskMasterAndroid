<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="UploadDocumentCell">
        <attr name="iconResId" format="reference"/>
        <attr name="actionResId" format="reference"/>
        <attr name="hintResId" format="reference"/>
    </declare-styleable>

    <declare-styleable name="TagsEditText">
        <attr name="allowSpaceInTag" format="boolean"/>
        <attr name="tagsTextColor" format="color"/>
        <attr name="tagsTextSize" format="dimension"/>
        <attr name="tagsBackground" format="integer"/>
        <attr name="tagsCloseImageLeft" format="integer"/>
        <attr name="tagsCloseImageRight" format="integer"/>
        <attr name="tagsCloseImagePadding" format="dimension" />
        <attr name="tagsPaddingLeft" format="dimension"/>
        <attr name="tagsPaddingRight" format="dimension"/>
        <attr name="tagsPaddingTop" format="dimension"/>
        <attr name="tagsPaddingBottom" format="dimension"/>
    </declare-styleable>

    <declare-styleable name="AspectRatioView">
        <attr name="view_aspectRatio" format="float"/>
    </declare-styleable>

    <declare-styleable name="MoneyEditText">
        <attr name="money_unit" format="string" />
    </declare-styleable>

    <attr name="bottomSheetDialogTheme" format="reference" />

    <declare-styleable name="AnimatedRecyclerView">
        <attr name="layoutAnimation" format="reference" />
        <attr name="animationDuration" format="integer" />
        <attr name="layoutManagerOrientation" format="integer">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="layoutManagerReverse" format="boolean" />
        <attr name="layoutManagerType" format="integer">
            <enum name="linear" value="0" />
            <enum name="grid" value="1" />
        </attr>
        <attr name="gridLayoutManagerColumns" format="integer" />
    </declare-styleable>

    <declare-styleable name="ElasticView">
        <attr name="view_scale" format="float" />
        <attr name="view_duration" format="integer" />
        <attr name="view_cornerRadius" format="dimension" />
    </declare-styleable>
    <declare-styleable name="ElasticButton">
        <attr name="button_scale" format="float" />
        <attr name="button_duration" format="integer" />
        <attr name="button_cornerRadius" format="dimension" />
    </declare-styleable>
    <declare-styleable name="ElasticCheckButton">
        <attr name="checkButton_scale" format="float" />
        <attr name="checkButton_duration" format="integer" />
        <attr name="checkButton_alpha" format="float" />
        <attr name="checkButton_isChecked" format="boolean" />
        <attr name="checkButton_cornerRadius" format="dimension" />
    </declare-styleable>
    <declare-styleable name="ElasticLayout">
        <attr name="layout_scale" format="float" />
        <attr name="layout_duration" format="integer" />
        <attr name="layout_cornerRadius" format="dimension" />
    </declare-styleable>
    <declare-styleable name="ElasticImageView">
        <attr name="imageView_scale" format="float" />
        <attr name="imageView_duration" format="integer" />
    </declare-styleable>
    <declare-styleable name="ElasticFloatingActionButton">
        <attr name="fabutton_scale" format="float" />
        <attr name="fabutton_duration" format="integer" />
    </declare-styleable>
    <declare-styleable name="ElasticCardView">
        <attr name="cardView_scale" format="float" />
        <attr name="cardView_duration" format="integer" />
    </declare-styleable>


    <declare-styleable name="CalendarPicker">
        <attr name="dayTextColor" format="color" />
        <attr name="yearTextColor" format="color" />
        <attr name="firstSelectedDateTextColor" format="color" />
        <attr name="secondSelectedDateTextColor" format="color" />
        <attr name="selectedDateCircleColor" format="color" />
        <attr name="eventDotColor" format="color" />
        <attr name="eventDotColorWhenHighlighted" format="color" />
        <attr name="eventDotColorWhenSelected" format="color" />
        <attr name="highlightedDatesBackgroundColor" format="color" />
        <attr name="highlightedDatesTextColor" format="color" />
        <attr name="titleColor" format="color" />
        <attr name="nextMonthButtonColor" format="color" />
        <attr name="previousMonthButtonColor" format="color" />
        <attr name="yearSelectButtonColor" format="color" />
        <attr name="yearSelectButtonIconColor" format="color" />
        <attr name="yearSelectButtonTextColor" format="color" />
        <attr name="dateRangeTextColor" format="color" />
        <attr name="daysOfWeekTextColor" format="color" />

        <attr name="nextMonthButtonIcon" format="reference" />
        <attr name="previousMonthButtonIcon" format="reference" />

        <attr name="eventDotGravity">
            <flag name="bottom" value="80" />
            <flag name="end" value="8388613" />
            <flag name="start" value="8388611" />
            <flag name="top" value="48" />
        </attr>
    </declare-styleable>

    <declare-styleable name="CustomView">
        <attr name="customBackgroundColor" format="color" />
        <attr name="customStrokeColor" format="color" />
        <attr name="customStrokeWidth" format="dimension" />
        <attr name="customCornerRadius" format="dimension" />
        <attr name="customElevation" format="dimension" />
    </declare-styleable>
</resources>